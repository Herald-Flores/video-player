{
  "name": "video-player",
  "version": "1.0.0",
  "description": "Basic video player with HTML, Tailwind and JavaScript",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "npm run build && vite preview",
    "lint": "eslint src/** --fix",
    "lint:modified": "eslint $(git diff --name-only --diff-filter=ACMRTUXB main | grep  -E \"(.js$)\") --fix",
    "format": "prettier --write src/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Herald-Flores/video-player.git"
  },
  "keywords": [
    "HTML",
    "JavaScript",
    "Tailwind",
    "Vite"
  ],
  "author": "Herald Flores",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Herald-Flores/video-player/issues"
  },
  "homepage": "https://github.com/Herald-Flores/video-player#readme",
  "devDependencies": {
    "autoprefixer": "10.4.13",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "postcss": "8.4.19",
    "postcss-nesting": "^10.2.0",
    "prettier": "2.8.0",
    "standard": "17.0.0",
    "stylelint": "14.15.0",
    "stylelint-config-standard": "29.0.0",
    "stylelint-prettier": "2.0.0",
    "tailwindcss": "3.2.4",
    "vite": "3.2.0",
    "vite-plugin-eslint": "1.8.1"
  },
  "type": "module"
}
